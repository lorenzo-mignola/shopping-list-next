generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model List {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime
  items     Item[]
}

model Item {
  id         Int       @id @default(autoincrement())
  name       String
  insertedAt DateTime
  updatedAt  DateTime?
  List       List?     @relation(fields: [listId], references: [id])
  listId     Int?
}
